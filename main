local memory = {}

function memory:createtable (index: number, size: number)
    memory[index] = table.create(size)
    return index,memory[index]
end

function memory:set (index: number, value: any)
    memory[index] = value
    return index,memory[index], value
end

function memory:create (index: number, class: any)
    memory[index] = Instance.new (class)
    return index,memory[index]
end

function memory:objset (index: number, subindex: number|string, value: any)
    memory[index][subindex] = value
    return index,memory[index]
end
